# Стивенсон, упаражнение 13
n = float(input('Введите сумму сдачи '))
print(n // 200, 'двухдолларовых монет')
n %= 200
print(n // 100, 'однодолларовых монет')
n %= 100
print(n // 25, '25–центовых монет')
n %= 25
print(n // 10, '10–центовых монет')
n %= 10
print(n // 5, '5–центовых монет')
print(n % 5, 'центов')
