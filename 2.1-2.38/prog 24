# Златопольский 2.21

e = float(input('Введите е '))
f = float(input('Введите f '))
g = float(input('Введите g '))
h = float(input('Введите h '))
from math import sqrt, sin
chis_a = e + f / 2 #Числитель для а
k = (g - h) ** 2 - 3 * sin(e) #Подкоренное выражение для с
if k >= 0:
    print('a =', chis_a / 3)
    print('b =', abs(h ** 2 - g))
    print('c =', sqrt(k))
else:
    print('Ошибка')
